<templateSet group="Liquid (HTML)">
  <template name="com" value="{% comment %}&#10;{#&#10;    $SELECTION$ $END$&#10;#}&#10;{% endcomment %}" description="{% comment %}...{% endcomment %}" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="img" value="{{ '$SELECTION$$END$' | asset_url }}" description="Liquid Image" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="CSS" value="true" />
      <option name="HTML" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSON" value="true" />
      <option name="PHP" value="true" />
      <option name="Twig" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
</templateSet>