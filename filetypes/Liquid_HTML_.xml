<filetype binary="false" description="Liquid" name="Liquid(HTML)">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="//" />
      <option name="COMMENT_START" value="{% comment %}" />
      <option name="COMMENT_END" value="{% endcomment %}" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
    </options>
    <keywords keywords="liquid" ignore_case="false" />
  </highlighting>
  <extensionMap>
    <mapping ext="liquid" />
  </extensionMap>
</filetype>