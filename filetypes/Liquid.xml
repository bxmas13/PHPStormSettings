<filetype binary="false" description="Shopify Liquid " name="Liquid">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="&lt;!--" />
      <option name="COMMENT_START" value="{% comment %}" />
      <option name="COMMENT_END" value="{% endcomment %}" />
      <option name="HEX_PREFIX" value="#" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
      <option name="LINE_COMMENT_AT_START" value="true" />
    </options>
    <keywords keywords="assign;capture;collection;endcapture;endfor;endif;for;if" ignore_case="false" />
  </highlighting>
  <extensionMap>
    <mapping ext="liquid" />
  </extensionMap>
</filetype>